<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
	<!-- Reference source code, if it exists, and the configuration is -source; 
  Otherwise, reference External binary files -->
	<Choose>
		<When Condition="'$(ReferenceSourceCode)' == 'true' And Exists('..\JdsuMicrosoftOfficeWrappers')">
			<ItemGroup>
				<ProjectReference Include="..\JdsuMicrosoftOfficeWrappers\JdsuMicrosoftOfficeWrappers.vbproj">
					<Project>{3898e572-84e3-4ce7-b313-e660df9826f5}</Project>
					<Name>JdsuMicrosoftOfficeWrappers</Name>
				</ProjectReference>
			</ItemGroup>
		</When>
		<When Condition="'$(ReferenceSourceCode)' == 'true' And Exists('..\LiteMZData\')">
			<ItemGroup>
				<Reference Include="JdsuMicrosoftOfficeWrappers">
					<HintPath>..\LiteMZData\Externals\JdsuMicrosoftOfficeWrappers\$(BinSubfolder)\JdsuMicrosoftOfficeWrappers.dll</HintPath>
				</Reference>
			</ItemGroup>
		</When>
		<Otherwise>
			<ItemGroup>
				<!-- Use binary reference included as externals to this project -->
				<Reference Include="JdsuMicrosoftOfficeWrappers">
					<HintPath>Externals\JdsuMicrosoftOfficeWrappers\$(BinSubfolder)\JdsuMicrosoftOfficeWrappers.dll</HintPath>
				</Reference>
			</ItemGroup>
		</Otherwise>
	</Choose>
</Project>