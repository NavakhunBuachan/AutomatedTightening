<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!-- Reference source code, if it exists, and the configuration is -source; 
  Otherwise, reference External binary files -->
  <Choose>
	<When Condition="'$(ReferenceSourceCode)' == 'true' And Exists('..\ASICamera')">
	  <ItemGroup>
		<ProjectReference Include="..\ASICamera\ASICamera.vbproj">
		  <Project>{A146233D-C721-4B30-83BE-A5777FF5C286}</Project>
		  <Name>ASICamera</Name>
		</ProjectReference>
	  </ItemGroup>
	</When>
	<Otherwise>
	  <ItemGroup>
		<Reference Include="ASICamera">
		  <HintPath>Externals\ASICamera\$(BinSubfolder)\ASICamera.exe</HintPath>
		</Reference>
	  </ItemGroup>
    </Otherwise>
  </Choose>
</Project>