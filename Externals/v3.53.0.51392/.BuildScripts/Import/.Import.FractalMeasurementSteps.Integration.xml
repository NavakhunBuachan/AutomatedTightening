<Project xmlns="http://schemas.microsoft.com/developer/msbuild/2003">
  <!-- Reference source code, if it exists, and the configuration is -source; 
  Otherwise, reference External binary files -->
  <Choose>
    <When Condition="'$(ReferenceSourceCode)' == 'true' And Exists('..\FractalMeasurementSteps_Integration\FractalMeasurementSteps.Integration.vbproj')">
      <ItemGroup>
		<ProjectReference Include="..\FractalMeasurementSteps_Integration\FractalMeasurementSteps.Integration.vbproj">
		  <Project>{D01DDB85-65C7-40A0-8B39-A5AD25FB6D14}</Project>
		  <Name>FractalMeasurementSteps.Integration</Name>
		</ProjectReference>
      </ItemGroup>
    </When>
    <Otherwise>
      <ItemGroup>
        <Reference Include="FractalMeasurementSteps.Integration">
          <HintPath>Externals\FractalMeasurementSteps_Integration\$(BinSubfolder)\FractalMeasurementSteps.Integration.dll</HintPath>
        </Reference>
      </ItemGroup>
    </Otherwise>
  </Choose>
</Project>